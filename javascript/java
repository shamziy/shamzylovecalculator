var loverName1 = prompt("what is your name?");
var loverName2 = prompt("what is their name?");


yourName = loverName1;

theirName = loverName2;

var loveScore = Math.random() * 100;
loveScore = Math.floor(loveScore) + 1;

if (loveScore > 89) {
  alert (yourName + " and " + theirName + " love for each other is " + loveScore + "% " + "that's unconditional ☺️🥳♥" );
}

if (loveScore > 69) {
    alert (yourName + " and " + theirName + " love for each other is  " + loveScore + "% " + "they love each other like Bees love honey🥺🤗❤️");
}

if (loveScore > 50 && loveScore < 70) {
    alert (yourName + " and " + theirName + " love score is " + loveScore + "% " + "that's average 🤔");
}


if (loveScore > 30 && loveScore <= 50) {
    alert (yourName + " and " + theirName + " love score is " + loveScore + "% " + "awwn love kid 😏😍😂");
}

if (loveScore <= 30) {
    alert(yourName + " and " + theirName + " love for each other is " + loveScore + "% " + "omo i no fit laugh abeg 😭😂");
    alert("This is not real. Love is a beautiful thing 🥰 I love you 😇❤️")
}
